<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LineFeedLayout">
        <attr name="horizontalSpacing" format="dimension"/>
        <attr name="verticalSpacing" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="VenusSwitchMaterial3">
        <!--switch-->
        <attr name="switchText" format="string" />
        <attr name="switchTextOn" format="string" />
        <attr name="switchTextOff" format="string" />
        <attr name="switchChecked" format="boolean" />
        <attr name="switchTextSize" format="dimension" />
        <!--button-->
        <attr name="buttonText" format="string" />
        <attr name="buttonIcon" format="reference" />
        <attr name="buttonTextSize" format="dimension" />
        <attr name="buttonVisible" format="boolean" />
    </declare-styleable>
</resources>